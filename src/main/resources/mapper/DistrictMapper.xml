<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ncy.store.mapper.DistrictMapper">
    <select id="findByParent" resultType="com.ncy.store.entity.District">
        select *
        from t_dict_district
        where parent = #{parent}
        order by code ASC
    </select>
    <select id="findNameByCode" resultType="java.lang.String">
        select name
        from t_dict_district
        where code = #{code}
    </select>
</mapper>